#!/bin/sh
mkdir -p ~/.newsboat
mkdir -p ~/.newsraft

newsboat_urls=$(op item get newsboat_urls --fields=notesPlain | sed 's/"//g')
cat <<EOF >~/.newsboat/urls
$newsboat_urls
EOF

newsraft_urls=$(op item get newsraft_urls --fields=notesPlain | sed 's/"//g')
cat <<EOF >~/.newsraft/feeds
$newsraft_urls
EOF
